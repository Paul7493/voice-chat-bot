# Voice Chat Bot Environment Variables
# Copy this file to .env and fill in your actual API credentials

# Dialogflow API Token
# Get this from your Dialogflow console or Google Cloud Console
APIAI_TOKEN=your_dialogflow_client_access_token_here

# Google Cloud API Key (optional, for additional features)
GOOGLE_CLOUD_API_KEY=your_google_cloud_api_key_here

# Unique session ID for Dialogflow conversations
# You can use any unique string identifier
APIAI_SESSION_ID=unique_session_id_12345

# Server port
PORT=5000
